.content-area-wrap {
    ul {
        margin-left: 0;
        padding-left: 0;

        @include desktop {
            columns: 3;
        }

        @include tablet {
            columns: 3;
        }

        li {
            list-style: none;
            margin-left: 0;
            padding-left: 0;
        }
    }
}